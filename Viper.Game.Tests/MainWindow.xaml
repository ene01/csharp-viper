<Window x:Class="Viper.Game.Tests.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Viper.Game.Tests"
        mc:Ignorable="d"
        Title="Viper.Game.Tests" Height="450" Width="800">
    <Grid x:Name="TestingMainGrid" Background="Black">
        <StackPanel x:Name="TestingSpacesButtons" HorizontalAlignment="Left" Width="200" Background="#FF0A0A0A"/>
        <Grid x:Name="TestingSpace" Margin="370,10,10,10" Background="#FF292929"/>
        <StackPanel x:Name="TestingInteractions" HorizontalAlignment="Left" Width="160" Background="#FF191919" Margin="200,0,0,0"/>

    </Grid>
</Window>
